            android:label="@string/title_activity_to_top"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".down_apk.DownActivity" />
        <activity android:name=".custom_view.RulerActivity" />
        <activity
            android:name=".head_move2.HeadMove2Activity"
            android:label="@string/title_activity_head_move2ctivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".recyclerview_anim.RecyclerViewAnimatorActivity" />
        <activity android:name=".head_move.HeadMoveActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>